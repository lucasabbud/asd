<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pedidosmovil2"
    android:versionCode="2"
    android:versionName="2.0" >
    
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
	
    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/invoice"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.pedidosmovil2.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity 
            android:name="com.pedidosmovil2.Customer"
            android:label="@string/customers" 
            android:icon="@drawable/users"
            android:windowSoftInputMode="stateHidden" >
        </activity>    
        <activity 
            android:name="com.pedidosmovil2.ventaDiaria"
            android:label="@string/ventaDiaria" 
            android:icon="@drawable/users"
            android:windowSoftInputMode="stateHidden" >
        </activity>  
        <activity 
            android:name="com.pedidosmovil2.Product"
            android:label="@string/products"
            android:icon="@drawable/products" 
            android:windowSoftInputMode="stateHidden" >
        </activity>
        <activity 
            android:name="com.pedidosmovil2.Invoice"
            android:label="@string/invoices"
            android:icon="@drawable/invoice" 
            android:windowSoftInputMode="stateHidden" >
        </activity>    
        <activity 
            android:name="com.pedidosmovil2.InvoiceAdd"
            android:label="@string/invoices"
            android:icon="@drawable/invoice" 
            android:windowSoftInputMode="stateHidden" >
        </activity>             
        <activity 
            android:name="com.pedidosmovil2.ItemDetail"
            android:label="@string/details" 
            android:windowSoftInputMode="stateHidden" >
        </activity> 
        <activity 
            android:name="com.pedidosmovil2.ItemAdd"
            android:label="@string/new_client" 
            android:windowSoftInputMode="stateHidden">
        </activity>
        <activity 
            android:name="com.pedidosmovil2.Options"
            android:label="@string/new_client" 
            android:windowSoftInputMode="stateHidden">
        </activity>
        <activity 
            android:name="com.pedidosmovil2.Signature"
            android:label="@string/new_client" 
            android:windowSoftInputMode="stateHidden">
        </activity>  
        <activity 
            android:name="com.pedidosmovil2.FileExplorer"
            android:label="Seleccione un archivo" 
            android:windowSoftInputMode="stateHidden">
        </activity>        
        
    </application>

</manifest>
